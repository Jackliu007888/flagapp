html, body
  height 100%
  width 100%
  font-size 40PX
  -webkit-overflow-scrolling touch
  -webkit-tap-highlight-color transparent
  font-family "Heiti SC", "Helvetica Neue", Helvetica, sans-serif

  #app
    width 100%
    height 100%
    overflow hidden

    @font-face {
      font-family: hanyi;
      src: url('http://static.ws.126.net/163/f2e/news/happynewyear2019/img/font.ttf')
    }

    @font-face {
      font-family: single;
      src: url('http://static.ws.126.net/163/f2e/news/happynewyear2019/img/eng.ttf')
    }

    @font-face {
      font-family: text;
      src: url('http://static.ws.126.net/163/f2e/news/happynewyear2019/img/text1.ttf')
    }